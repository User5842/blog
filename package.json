{
  "name": "blog",
  "version": "1.0.0",
  "description": "A collection of my thoughts.",
  "private": true,
  "scripts": {
    "build": "npm run lint && rimraf built && tsc && npm run copy && npm run less",
    "copy": "copyfiles -e src/public/scripts/**/* -u 1 src/public/**/* src/views/**/* built",
    "dev": "nodemon built/server.js",
    "less": "lessc src/styles/bundle.less built/public/styles/bundle.css",
    "lint": "eslint . --ext .js,.ts",
    "start": "node built/server.js",
    "watch": "tsc -w"
  },
  "keywords": [],
  "author": "Rafael Negron",
  "license": "ISC",
  "dependencies": {
    "compression": "1.7.4",
    "dotenv": "16.0.0",
    "ejs": "3.1.6",
    "express": "4.17.3",
    "helmet": "5.0.2",
    "marked": "4.0.10",
    "mongoose": "6.2.3",
    "node-fetch": "2.6.7",
    "pm2": "5.2.0"
  },
  "devDependencies": {
    "@types/compression": "1.7.2",
    "@types/express": "4.17.13",
    "@types/marked": "4.0.1",
    "@types/node": "16.11.26",
    "@types/node-fetch": "2.6.1",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "copyfiles": "2.4.1",
    "eslint": "8.9.0",
    "less": "4.1.2",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "typescript": "4.5.5"
  }
}
